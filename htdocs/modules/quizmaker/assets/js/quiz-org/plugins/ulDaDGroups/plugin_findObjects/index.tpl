  function getTplNewClass2 (currentQuestion, chrono){

  var obj;

    switch (currentQuestion.type){
                    case "alphaSimple" : obj = getPlugin_alphaSimple(currentQuestion, chrono); break;
                    case "findObjects" : obj = getPlugin_findObjects(currentQuestion, chrono); break;
                    case "imagesColor" : obj = getPlugin_imagesColor(currentQuestion, chrono); break;
                    case "imagesDaDGroups" : obj = getPlugin_imagesDaDGroups(currentQuestion, chrono); break;
                    case "imagesDaDMatchItems" : obj = getPlugin_imagesDaDMatchItems(currentQuestion, chrono); break;
                    case "imagesLogical" : obj = getPlugin_imagesLogical(currentQuestion, chrono); break;
                    case "imagesSimple" : obj = getPlugin_imagesSimple(currentQuestion, chrono); break;
                    case "listboxClassItems" : obj = getPlugin_listboxClassItems(currentQuestion, chrono); break;
                    case "matchItems" : obj = getPlugin_matchItems(currentQuestion, chrono); break;
                    case "multiQuestions" : obj = getPlugin_multiQuestions(currentQuestion, chrono); break;
                    case "pageBegin" : obj = getPlugin_pageBegin(currentQuestion, chrono); break;
                    case "pageEnd" : obj = getPlugin_pageEnd(currentQuestion, chrono); break;
                    case "pageGroup" : obj = getPlugin_pageGroup(currentQuestion, chrono); break;
                    case "pageInfo" : obj = getPlugin_pageInfo(currentQuestion, chrono); break;
                    case "radioMultiple" : obj = getPlugin_radioMultiple(currentQuestion, chrono); break;
                    case "selectImages" : obj = getPlugin_selectImages(currentQuestion, chrono); break;
                    case "selectInputs" : obj = getPlugin_selectInputs(currentQuestion, chrono); break;
                    case "sortItems" : obj = getPlugin_sortItems(currentQuestion, chrono); break;
                    case "textareaMixte" : obj = getPlugin_textareaMixte(currentQuestion, chrono); break;
                    case "ulDaDGroups" : obj = getPlugin_ulDaDGroups(currentQuestion, chrono); break;
        
    default: alert("getTplNewClass - Classe absente : " + currentQuestion.type); break;
    }

    //blob("getTplNewClass - Classe : " + currentQuestion.type);
    return obj;
}
</script>

    
     

